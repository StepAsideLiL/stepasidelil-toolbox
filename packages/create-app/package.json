{
  "name": "@stepasidelil/create-app",
  "version": "0.0.1",
  "description": "Create a Nextjs app with some default configuration.",
  "author": "",
  "bin": {
    "@stepasidelil/create-app": "./dist/index.js"
  },
  "files": [
    "dist",
    "package.json",
    "README.md"
