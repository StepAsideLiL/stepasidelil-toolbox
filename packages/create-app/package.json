    "node": ">=20"
  },
  "devDependencies": {
    "@types/node": "^22.13.1",
    "@vercel/ncc": "^0.38.3",
    "commander": "^13.1.0",
    "cross-spawn": "^7.0.6",
    "fast-glob": "^3.3.3",
    "fs-extra": "^11.3.0",
    "prompts": "^2.4.2",
    "typescript": "5.7.3"
  }
